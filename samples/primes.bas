10 REM PRIME NUMBER FINDER
20 REM FINDS PRIMES UP TO N
30 HOME
40 PRINT "PRIME NUMBER FINDER"
50 PRINT "==================="
60 PRINT
70 N = 100
80 PRINT "PRIMES UP TO "; N; ":"
90 PRINT
100 FOR I = 2 TO N
110 ISPRIME = 1
120 FOR J = 2 TO SQR(I)
130 IF INT(I / J) * J = I THEN ISPRIME = 0
140 NEXT J
150 IF ISPRIME = 1 THEN PRINT I; " ";
160 NEXT I
170 PRINT
180 PRINT
190 PRINT "DONE!"
200 END
